pipeline {
  agent any
  tools {
    maven 'mvn'
  }
  stages {
    stage('Build') {
      steps {
        sh 'echo hello'
      }
    }
stage ( 'SCM CHECKOUT')
{
git 'https://github.com/kusupudiswamy/maven-hello-world.git'
}
  stage ('compile-package'){
    sh '''
    whoami
    
    pwd
    cd my-app
    
    pwd
    
    mvn -B -DskipTests clean package'''

}
  }
}
